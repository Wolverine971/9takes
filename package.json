		"autoprefixer": "^10.4.13",
		"axios": "^1.5.1",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.10.6",
		"node-sass": "^8.0.0",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"postcss-preset-env": "^7.8.3",
		"postcss-simple-vars": "^7.0.1",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"sass": "^1.58.3",
		"stopword": "^2.0.7",
		"stylelint": "^15.10.3",
