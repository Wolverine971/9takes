{
	"name": "9takes",
	"version": "0.0.1",
	"packageManager": "pnpm@7.16.1",
	"engines": {
		"node": ">=16",
		"pnpm": ">=7.13"
	},
	"scripts": {
		"dev": "vite dev",
		"devTest": "vavite-loader vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"all:install": "pnpm -w i --filter=\"*\"",
		"all:build": "pnpm -w exec nx run-many --target=build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"postbuild": "pnpm svelte-sitemap --domain https://9takes.com",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@playwright/test": "^1.28.1",
		"@supabase/auth-helpers-sveltekit": "^0.8.6",
		"@supabase/supabase-js": "^2.1.3",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-vercel": "^2.4.1",
		"@sveltejs/kit": "^1.0.0",
		"@types/cookie": "^0.5.1",
		"@types/gtag.js": "^0.0.12",
		"@types/stopword": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.2",
		"axios": "^1.2.2",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.10.6",
		"node-sass": "^8.0.0",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"postcss-preset-env": "^7.8.3",
		"postcss-simple-vars": "^7.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"sass": "^1.57.1",
		"stopword": "^2.0.5",
		"supabase": "^1.34.5",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"@elastic/elasticsearch": "^8.6.0",
		"@sveltejs/adapter-node": "^1.2.0",
		"@vavite/node-loader": "^1.7.0",
		"@vercel/analytics": "^0.1.11",
		"dotenv": "^16.0.3",
		"googleapis": "^114.0.0",
		"mermaid": "^9.3.0",
		"natural": "^6.2.0",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-slug": "^5.1.0",
		"remark-abbr": "^1.4.1",
		"remark-github": "^11.2.4",
		"stemmer": "^2.0.1",
		"stripe": "^11.17.0",
		"web-vitals": "^3.3.0"
	}
}
