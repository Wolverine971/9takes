{
	"name": "9takes",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"devTest": "vavite-loader vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"all:install": "pnpm -w i --filter=\"*\"",
		"all:build": "pnpm -w exec nx run-many --target=build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"clean": "rimraf .svelte-kit",
		"gen:sctx": "tsx docs/development/generateStreamlinedProjectContext2.ts . -o ./sproject-context.json",
		"label-paths": "tsx docs/development/labelFilePaths.ts",
		"gen:types": "node scripts/generate-types.js",
		"gen:sitemap": "node scripts/generate-sitemap.js",
		"gen:famous-types": "node scripts/generate-famous-types.js",
		"gen:crosslinks": "node scripts/generate-crosslink-report.js",
		"gen:all": "pnpm format && pnpm gen:types && pnpm gen:famous-types && pnpm gen:sitemap && pnpm gen:crosslinks",
		"index:blogs": "node scripts/index-blogs-to-supabase.js",
		"index:blogs:dry": "node scripts/index-blogs-to-supabase.js --dry-run",
		"index:blogs:force": "node scripts/index-blogs-to-supabase.js --force"
	},
	"devDependencies": {
		"@babel/parser": "^7.27.4",
		"@babel/traverse": "^7.27.4",
		"@babel/types": "^7.27.3",
		"@playwright/test": "^1.47.1",
		"@sveltejs/adapter-vercel": "^6.3.0",
		"@sveltejs/enhanced-img": "^0.9.2",
		"@sveltejs/kit": "^2.49.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/typography": "^0.5.14",
		"@types/gtag.js": "^0.0.12",
		"@types/pluralize": "^0.0.30",
		"@types/qrcode": "^1.5.5",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^3.14.0",
		"fast-glob": "^3.3.3",
		"flowbite": "^2.5.1",
		"mdsvex": "^0.12.6",
		"postcss": "^8.4.47",
		"postcss-load-config": "^4.0.2",
		"postcss-preset-env": "^7.8.3",
		"postcss-simple-vars": "^7.0.1",
		"prettier": "^3.0",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"sass": "^1.79.2",
		"supabase": "^1.192.5",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^3.4.9",
		"tslib": "^2.7.0",
		"tsx": "^4.19.4",
		"typescript": "^5.6.2",
		"vite": "^7.3.1",
		"vitest": "^4.0.16"
	},
	"browser": {
		"crypto": false
	},
	"type": "module",
	"engines": {
		"node": ">=22.0.0",
		"pnpm": ">=8.0.0"
	},
	"dependencies": {
		"@elastic/elasticsearch": "^8.15.0",
		"@fingerprintjs/fingerprintjs": "^4.5.0",
		"@supabase/auth-helpers-sveltekit": "^0.8.7",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.45.4",
		"@vavite/node-loader": "^1.8.3",
		"aws-sdk": "^2.1691.0",
		"axios": "^1.7.7",
		"base64-arraybuffer": "^1.0.2",
		"cheerio": "^1.0.0",
		"d3": "^7.9.0",
		"d3-cloud": "^1.2.7",
		"dotenv": "^16.4.5",
		"flowbite-svelte": "^0.46.16",
		"googleapis": "^114.0.0",
		"gray-matter": "^4.0.3",
		"html-to-image": "^1.11.11",
		"html2canvas": "^1.4.1",
		"jspdf": "^3.0.1",
		"mermaid": "^11.2.1",
		"pluralize": "^8.0.0",
		"postcss-scss": "^4.0.9",
		"qrcode": "^1.5.4",
		"rehype-slug": "^5.1.0",
		"remark-abbr": "^1.4.2",
		"remark-github": "^11.2.4",
		"stemmer": "^2.0.1",
		"stripe": "^11.18.0",
		"uuid": "^9.0.1",
		"web-vitals": "^3.5.2",
		"zod": "^3.25.76"
	}
}
